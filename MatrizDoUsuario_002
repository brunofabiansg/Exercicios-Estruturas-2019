#include<stdio.h>

main (){
	int i, j, a, b;
	int matriz [i][j];
	
	printf("Digite quantidade de linhas:");
	scanf("%d", &i);
	printf("Digite quantidade de colunas:");
	scanf("%d", &j);
	
	printf ("\nOrdem da Matriz:[%d][%d]", i, j );
	for ( a=1; a<=i; a++ ){
		printf("\n");
	  	for ( b=1; b<=j; b++ ){
	  		printf("a%d%d ", a,b);
	  	}
	}
	printf("%d", matriz[1][1]);
}
